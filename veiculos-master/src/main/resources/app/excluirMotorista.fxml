<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ExcluirMotorista">
   <children>
      <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" onAction="#voltarAoMenu" text="Menu">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#voltarAoMenu" text="Menu" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane prefHeight="344.0" prefWidth="640.0">
         <children>
            <Label layoutX="216.0" layoutY="107.0" prefHeight="13.0" prefWidth="175.0" text="Selecione o motorista ">
               <font>
                  <Font name="Times New Roman" size="19.0" />
               </font>
            </Label>
            <ComboBox fx:id="comboMotoristas" layoutX="149.0" layoutY="160.0" prefHeight="14.0" prefWidth="308.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </ComboBox>
            <Label layoutX="200.0" layoutY="21.0" text="Excluir Motorista">
               <font>
                  <Font name="Times New Roman" size="29.0" />
               </font>
               <effect>
                  <Glow level="0.39" />
               </effect>
            </Label>
            <Button fx:id="botaoCancelar" layoutX="200.0" layoutY="243.0" mnemonicParsing="false" onAction="#voltarAoMenu" text="Cancelar">
               <font>
                  <Font name="Times New Roman" size="15.0" />
               </font>
               <effect>
                  <Blend />
               </effect>
            </Button>
            <Button layoutX="341.0" layoutY="243.0" mnemonicParsing="false" onAction="#excluirMotorista" text="Excluir">
               <font>
                  <Font name="Times New Roman" size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
