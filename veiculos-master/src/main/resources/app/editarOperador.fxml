<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.EditarOperador{">
   <children>
      <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="Menu">
               <items>
                  <MenuItem mnemonicParsing="false" text="Menu" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="239.0" layoutY="14.0" text="Editar Operador">
               <font>
                  <Font name="Times New Roman" size="30.0" />
               </font>
            </Label>
            <Button layoutX="235.0" layoutY="396.0" mnemonicParsing="false" onAction="#voltarAoMenu" text="Cancelar">
               <font>
                  <Font name="Times New Roman" size="15.0" />
               </font>
            </Button>
            <Button layoutX="371.0" layoutY="396.0" mnemonicParsing="false" onAction="#editarOperador" prefHeight="26.0" prefWidth="62.0" text="Salvar">
               <font>
                  <Font name="Times New Roman" size="15.0" />
               </font>
            </Button>
            <Label layoutX="220.0" layoutY="56.0" text="Altere as informações que deseja que sejam alteradas.">
               <font>
                  <Font name="Times New Roman" size="12.0" />
               </font>
            </Label>
            <Label layoutX="167.0" layoutY="96.0">
               <font>
                  <Font name="Times New Roman" size="15.0" />
               </font>
            </Label>
            <ComboBox fx:id="comboOperador" layoutX="261.0" layoutY="102.0" prefWidth="150.0" promptText="Selecione Operador" />
            <Label layoutX="259.0" layoutY="144.0" text="Nome do operador:">
               <font>
                  <Font name="Times New Roman" size="20.0" />
               </font>
            </Label>
            <Label layoutX="267.0" layoutY="215.0" text="Novo endereço:">
               <font>
                  <Font name="Times New Roman" size="20.0" />
               </font>
            </Label>
            <Label layoutX="208.0" layoutY="300.0" text="Login:">
               <font>
                  <Font name="Times New Roman" size="20.0" />
               </font>
            </Label>
            <Label layoutX="379.0" layoutY="300.0" text="Nova senha:">
               <font>
                  <Font name="Times New Roman" size="20.0" />
               </font>
            </Label>
            <TextField fx:id="campoNome" layoutX="262.0" layoutY="178.0" />
            <TextField fx:id="campoEndereco" layoutX="262.0" layoutY="254.0" />
            <TextField fx:id="campoLogin" layoutX="160.0" layoutY="323.0" />
            <TextField fx:id="campoSenha" layoutX="355.0" layoutY="323.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
